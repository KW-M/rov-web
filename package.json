{
  "name": "rov-web",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@8.12.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run dev:frontend",
    "build": "pnpm run build:frontend; pnpm run build:internal;",
    "preview": "pnpm run preview:frontend;",
    "dev:frontend": "cd frontend; vite dev --host",
    "dev:internal": "cd rov-internal-website; vite dev --host",
    "build:frontend": "cd frontend; vite build",
    "build:internal": "cd rov-internal-website; vite build",
    "preview:frontend": "cd frontend; vite preview --host",
    "preview:internal": "cd rov-internal-website; vite preview --host",
    "sveltekit:sync": "cd frontend; svelte-kit sync",
    "sveltekit:check": "cd frontend;svelte-kit sync && svelte-check --tsconfig ./frontend/tsconfig.json",
    "optimize:svgs": "svgo --folder frontend/src/assets/originals --output frontend/src/assets/optimized",
    "compile:proto": "pnpm run compile:proto:python && pnpm run compile:proto:javascript",
    "compile:proto:python": "protoc --python_betterproto_out=rov-python/protobufs/ protobufs/rov_actions.proto",
    "compile:proto:javascript": "pbjs -t static-module --es6 -w es6 -o frontend/src/js/shared/protobufs/rovActionsProto.js protobufs/rov_actions.proto;pbts -o frontend/src/js/shared/protobufs/rovActionsProto.d.ts frontend/src/js/shared/protobufs/rovActionsProto.js",
    "compile:browserify": "browserify ./src/js/shared/etc/nodeJsShims.js --standalone nodeJsShim -o ./src/js/shared/nodeShimsBundle.js",
    "compile:cython": "python3 rov-python/cython_modules/setup.py build_ext --inplace",
    "clean": "rm -r ./rov-internal-website/build; rm -r ./frontend/build; rm -r ./frontend/.svelte-kit; rm -rf ./node_modules; rm ./pnpm-lock.yaml",
    "run_py": "python3 rov-python/main.py --config-file  tooling/rasberry_pi_setup_scripts/new_config_files/rov-config.example.json"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "2.5.0",
    "@skeletonlabs/tw-plugin": "0.2.4",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.27.5",
    "@tailwindcss/typography": "0.5.10",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "20.9.0",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.31",
    "protobufjs-cli": "~1.1.2",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "svelte-google-materialdesign-icons": "^0.8.2",
    "svelte-preprocess": "^5.1.0",
    "svgo": "^3.0.2",
    "tailwindcss": "3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.1.1",
    "vite-plugin-node-polyfills": "~0.16.0",
    "vite-plugin-require": "^1.1.11",
    "vite-plugin-tailwind-purgecss": "0.1.3"
  },
  "dependencies": {
    "@floating-ui/dom": "1.5.3",
    "axios": "^1.6.2",
    "jose": "^5.1.1",
    "jsonwebtoken": "9.0.0",
    "livekit-client": "1.9.7",
    "livekit-server-sdk": "1.2.7",
    "protobufjs": "~7.2.3",
    "@thaunknown/simple-peer": "^9.12.1",
    "simple-peer": "^9.11.1",
    "twgl.js": "^5.3.1",
    "uuid": "^9.0.0",
    "virtual-gamepad-lib": "^0.0.4",
    "ws": "^8.13.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "livekit-server-sdk@1.2.7": "patches/livekit-server-sdk@1.2.7.patch"
    }
  }
}
