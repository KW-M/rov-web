{
  "name": "rov-web",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "compile:proto": "pnpm run compile:proto:python && pnpm run compile:proto:javascript",
    "compile:proto:python": "protoc --python_betterproto_out=rov-backend/python/protobufs/ shared/protobufs/rov_actions.proto",
    "compile:proto:javascript": "pbjs -t static-module --es6 -w es6 -o shared/js/protobufs/rovActionsProto.js shared/protobufs/rov_actions.proto;pbts -o shared/js/protobufs/rovActionsProto.d.ts shared/js/protobufs/rovActionsProto.js",
    "optimize:svgs": "svgo --folder ./frontend/src/assets/originals --output ./frontend/src/assets/optimized",
    "dev": "vite --host",
    "build": "vite build --base=./",
    "preview": "vite preview --host --base=/rov-web/",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rgossiaux/svelte-headlessui": "^1.0.2",
    "@steeze-ui/heroicons": "^2.2.2",
    "@steeze-ui/material-design-icons": "1.0.0",
    "@steeze-ui/svelte-icon": "^1.3.2",
    "@sveltejs/vite-plugin-svelte": "^2.2.0",
    "@tsconfig/svelte": "^4.0.1",
    "@xstate/fsm": "^2.0.0",
    "@xstate/inspect": "^0.8.0",
    "@zerodevx/svelte-toast": "^0.9.3",
    "autoprefixer": "^10.4.14",
    "buffer": "^6.0.3",
    "peerjs": "^1.4.7",
    "postcss": "^8.4.19",
    "protobufjs-cli": "~1.1.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^5.0.3",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-mp": "^2.2.3",
    "vite-plugin-multi-pages": "^0.0.10",
    "vite-plugin-pwa": "^0.15.0",
    "vite-plugin-resolve": "^2.3.0",
    "workbox-build": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "ws": "^8.13.0",
    "xstate": "^4.37.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "daisyui": "^2.51.6",
    "glob": "^10.2.6",
    "livekit-client": "^1.9.6",
    "livekit-server-sdk": "^1.2.1",
    "protobufjs": "~7.2.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "simple-peer": "^9.11.1",
    "twgl.js": "^5.3.1",
    "uuid": "^9.0.0",
    "virtual-gamepad-lib": "^0.0.4",
    "workbox-core": "^6.5.4"
  }
}
