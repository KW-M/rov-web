{
  "name": "rov-web",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "compile:proto": "pnpm run compile:proto:python && pnpm run compile:proto:javascript",
    "compile:proto:python": "protoc --python_betterproto_out=rov-backend/python/protobufs/rov_actions shared/protobufs/rov_actions.proto",
    "compile:proto:javascript": "pbjs -t static-module --es6 -w es6 -o shared/js/protobufs/rovActionsCompiled.js shared/protobufs/rov_actions.proto;pbts -o shared/js/protobufs/rovActionsCompiled.d.ts shared/js/protobufs/rovActionsCompiled.js",
    "optimize:svgs": "svgo --folder ./frontend/src/assets/originals --output ./frontend/src/assets/optimized",
    "dev": "vite --host",
    "build": "vite build --base=./",
    "preview": "vite preview --host --base=/rov-web/",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rgossiaux/svelte-headlessui": "^1.0.2",
    "@steeze-ui/heroicons": "^2.2.1",
    "@steeze-ui/material-design-icons": "1.0.0",
    "@steeze-ui/svelte-icon": "^1.3.2",
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@xstate/fsm": "^2.0.0",
    "@xstate/inspect": "^0.7.0",
    "@zerodevx/svelte-toast": "^0.8.2",
    "autoprefixer": "^10.4.13",
    "peerjs": "^1.4.7",
    "postcss": "^8.4.23",
    "protobufjs-cli": "~1.0.2",
    "svelte": "^3.54.0",
    "svelte-check": "^2.10.0",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^4.10.7",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "ws": "^8.9.0",
    "xstate": "^4.35.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "daisyui": "^2.43.0",
    "livekit-client": "^1.6.9",
    "protobufjs": "~7.1.2",
    "twgl.js": "^5.3.0",
    "uuid": "^9.0.0",
    "virtual-gamepad-lib": "^0.0.4"
  }
}
